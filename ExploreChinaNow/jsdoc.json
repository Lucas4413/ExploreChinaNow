{
  "source": {
    "include": ["src"], 
    "includePattern": ".jsx?$", 
    "excludePattern": "(node_modules|test)" 
  },
  "opts": {
    "destination": "./docs", 
    "recurse": true, 
    "template": "node_modules/jsdoc/templates/default"
  },
  "templates": {
    "cleverLinks": true, 
    "monospaceLinks": true
  }
}
